    },
    {
      "type": "v2rayGeoIPDat",
      "action": "output",
      "args": {
        "outputDir": "./output",
        "wantedList": ["cn", "private", "telegram"],
        "outputName": "geoip-lite.dat"
      }
    },
    {
      "type": "maxmindMMDB",
      "action": "output",
      "args": {
        "outputDir": "./output",
        "outputName": "Country.mmdb"
      }
    },
    {
      "type": "maxmindMMDB",
      "action": "output",
      "args": {
        "wantedList": ["cn", "private", "telegram"],
        "outputDir": "./output",
        "outputName": "Country-Lite.mmdb"
      }
    }
  ]
}
